# Ensure mod_rewrite is enabled on Apache
<IfModule mod_rewrite.c>
  RewriteEngine On

  # Redirecionar raiz do site para /home (temporário para evitar cache durante testes)
  RewriteRule ^$ home [L,R=302]
</IfModule>

# Fallback caso mod_rewrite não esteja disponível (também temporário)
RedirectMatch 302 ^/$ /home