{
  "version": 2,
  "name": "site-bancas-do-bairro",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BASE_URL": "@next_public_base_url",
    "NEXT_PUBLIC_GOOGLE_MAP_KEY": "@next_public_google_map_key",
    "NEXT_CLIENT_HOST_URL": "@next_client_host_url",
    "NEXT_PUBLIC_SITE_VERSION": "@next_public_site_version"
  },
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 30
    }
  }
}
